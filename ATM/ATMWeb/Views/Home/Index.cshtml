@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Excel Bank</h1>
    <p class="lead"> Excel Bank is a the fisrt financial institution that offers the first-ever web-based ATM</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Powered by DotNet Framework</a></p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Register</h2>
        <p>
            Open an account with us
        </p>
        <p>@Html.ActionLink("Register", "Register", "Account", null, new { @class = "btn btn-default" })</p>
    </div>
    <div class="col-md-4">
        <h2>Login</h2>
        <p>Access you existing account</p>
        <p>@Html.ActionLink("Log in", "Login", "Account", null, new { @class = "btn btn-default" })</p>
    </div>

</div>